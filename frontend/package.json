{
  "name": "peloton-reservations",
  "description": "Frontend code for interacting with Peloton schedule and user account",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "service-worker": "npm run build && ln build/service-worker.js public/service-worker.js",
    "sentry-sourcemaps": "sentry-cli sourcemaps inject --org titus-design --project peloton-reservations ./build && sentry-cli sourcemaps upload --org titus-design --project peloton-reservations ./build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.9.0",
    "@sentry/react": "10.11.0",
    "firebase": "12.2.1",
    "firebaseui": "6.1.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-redux": "9.2.0",
    "react-router": "7.9.1",
    "react-router-dom": "7.9.1",
    "shared": "file:../shared",
    "styled-components": "6.1.19",
    "web-vitals": "5.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@sentry/cli": "2.53.0",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "30.0.0",
    "@types/node": "24.5.0",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "react-scripts": "5.0.1"
  }
}
